{
  "name": "wizaplace/elastic-apm-symfony",
  "type": "symfony-bundle",
  "description": "Wizaplace Elastic APM Wrapper for Symfony",
  "keywords": [
    "Elastic APM"
  ],
  "homepage": "https://github.com/wizaplace/elastic-apm-symfony",
  "license": "proprietary",
  "authors": [
    {
      "name": "Wizacha DevTeam",
      "email": "dev@wizacha.com"
    }
  ],
  "config": {
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Wizacha\\ElasticApmBundle\\": "src/"
    },
    "exclude-from-classmap": [
      "/Tests/"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Wizacha\\ElasticApmBundle\\Tests\\": "tests/"
    }
  },
  "require": {
    "philkra/elastic-apm-php-agent": "dev-master",
    "symfony/dependency-injection": "~3.0",
    "symfony/symfony": ">=3.0 <4.3",
    "wizaplace/elastic-apm-wrapper": "^0.4.0"
  },
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/wizaplace/elastic-apm-wrapper.git"
    }
  ],
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.16",
    "infection/infection": "^0.15.3",
    "phpstan/extension-installer": "^1.0",
    "phpstan/phpstan": "^0.12.27",
    "phpstan/phpstan-deprecation-rules": "^0.12.4",
    "phpstan/phpstan-strict-rules": "^0.12.2",
    "phpunit/phpunit": "^8.5",
    "squizlabs/php_codesniffer": "^3.5"
  }
}
